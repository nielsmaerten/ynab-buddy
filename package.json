{
  "name": "ynab-buddy",
  "version": "3.0.0-preview.4",
  "description": "Upload & import CSV files from any bank into YNAB",
  "keywords": [
    "ynab",
    "youneedabudget",
    "csv",
    "import"
  ],
  "main": "./dist/index.js",
  "bin": {
    "ynab-buddy": "./dist/index.js"
  },
  "files": [
    "dist/",
    "assets/"
  ],
  "license": "MIT",
  "author": {
    "name": "Niels Maerten",
    "url": "https://github.com/nielsmaerten"
  },
  "homepage": "https://github.com/nielsmaerten/ynab-buddy",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nielsmaerten/ynab-buddy.git"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "hooks": "bun x cross-env NODE_ENV=hooks bun run src/index.ts",
    "typecheck": "bun x tsc --noEmit",
    "clean": "rimraf dist",
    "build": "bun run clean && bun x tsc",
    "build:bin:linux": "bun run scripts/build-bin.ts linux",
    "build:bin:macos": "bun run scripts/build-bin.ts macos",
    "build:bin:win": "bun run scripts/build-bin.ts win",
    "build:bin": "bun run scripts/build-bin.ts",
    "package": "bun run build && bun run build:bin",
    "prepublishOnly": "bun run package && bun test --coverage --max-concurrency=1 src",
    "test": "bun test --coverage --max-concurrency=1 src",
    "test:integration": "bun run scripts/docker-integration.ts",
    "lint:fix": "bun x prettier ./src --write",
    "lint": "bun x prettier ./src --check"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/luxon": "^3.2.0",
    "@types/node": "^20.0.0",
    "@types/pako": "^2",
    "@types/prompts": "^2.4.3",
    "bun-types": "^1.3.4",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "np": "^10.0.0",
    "prettier": "3.7.4",
    "rimraf": "^6.1.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "chalk": "^4.1.2",
    "csv-parse": "^4.16.3",
    "csv-stringify": "^5.6.5",
    "glob": "^10.2.2",
    "js-yaml": "^4.1.0",
    "luxon": "^3.3.0",
    "minimatch": "^9.0.0",
    "pako": "^2.1.0",
    "prompts": "^2.4.2",
    "ynab": "2.10.0"
  },
  "resolutions": {
    "minimist": "^1.2.7"
  },
  "packageManager": "bun@1.3.4",
  "np": {
    "cleanup": false
  }
}
